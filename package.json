{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.6.12",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.41.0",
    "@types/node": "^20.5.6",
    "@vitejs/plugin-vue": "^4.3.3",
    "eslint": "^8.47.0",
    "lerna": "^7.1.5",
    "pnpm": "^8.6.12",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "antfu/import-dedupe": "off",
      "antfu/no-cjs-exports": "off",
      "antfu/no-ts-export-equal": "off",
      "no-console": "off"
    }
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.3.9"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.0",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "eslint": "^8.54.0",
    "lerna": "^8.0.0",
    "pnpm": "^8.11.0",
    "taze": "^0.12.1",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.2",
    "vue-tsc": "^1.8.22"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "antfu/import-dedupe": "off",
      "antfu/no-cjs-exports": "off",
      "antfu/no-ts-export-equal": "off",
      "no-console": "off"
    }
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

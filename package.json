{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.6.5",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.39.5",
    "@types/node": "^20.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.43.0",
    "lerna": "^7.1.0",
    "pnpm": "^8.6.5",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.2"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "antfu/import-dedupe": "off",
      "antfu/no-cjs-exports": "off",
      "antfu/no-ts-export-equal": "off",
      "no-console": "off"
    }
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

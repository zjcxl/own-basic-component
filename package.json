{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.14.3",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@types/node": "^20.11.7",
    "@vitejs/plugin-vue": "^5.0.3",
    "eslint": "^8.56.0",
    "lerna": "^8.0.2",
    "pnpm": "^8.14.3",
    "taze": "^0.13.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

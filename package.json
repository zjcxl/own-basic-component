{
  "name": "@own-team/own-basic-component",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "version": "0.8.139",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "bumpp": "bumpp patch -r --commit --push --tag -y",
    "publish:release": "pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -r -f -w"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.21.2",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.10",
    "bumpp": "^9.4.1",
    "eslint": "8.57.0",
    "esno": "^4.7.0",
    "pnpm": "^9.4.0",
    "rimraf": "^5.0.7",
    "taze": "^0.13.9",
    "typescript": "^5.5.2",
    "unbuild": "^2.0.0",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

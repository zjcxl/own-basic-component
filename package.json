{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@7.21.0",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore ."
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.38.5",
    "@types/node": "^18.16.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.39.0",
    "lerna": "^6.6.1",
    "pnpm": "^8.3.1",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vite": "^4.3.1",
    "vue-tsc": "^1.4.4"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "antfu/import-dedupe": "off",
      "no-console": "off"
    }
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

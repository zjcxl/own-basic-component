{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.15.5",
  "version": "0.7.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "publish:release": "nr build && bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --no-git-checks --access public",
    "publish:pre": "nr build",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.9.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "bumpp": "^9.4.0",
    "eslint": "^8.57.0",
    "esno": "^4.7.0",
    "pnpm": "^8.15.5",
    "rimraf": "^5.0.5",
    "taze": "^0.13.3",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vite": "^5.2.2",
    "vitest": "^1.4.0"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}

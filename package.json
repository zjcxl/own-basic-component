{
  "name": "@own-team/base",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "version": "0.0.1",
  "scripts": {
    "build": "pnpm -r run build",
    "build:stub": "pnpm -r run build:stub",
    "publish:beta": "lerna publish --dist-tag beta",
    "publish:release": "lerna publish --dist-tag latest",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "up": "taze major -r -I"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.3.10"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.2",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "eslint": "^8.55.0",
    "lerna": "^8.0.0",
    "pnpm": "^8.11.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.6",
    "vue-tsc": "^1.8.25"
  },
  "workspaces": [
    "playground",
    "packages/*"
  ]
}
